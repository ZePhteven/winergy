{
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "lf",
  "importOrder": ["^@nestjs/(.*)", "<THIRD_PARTY_MODULES>", "^src/(.*)", "^test/(.*)", "^[./]"],
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
